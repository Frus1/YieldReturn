<Window x:Class="YieldReturn.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YieldReturn"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="StartTextBox" GotFocus="StartTextBox_GotFocus" HorizontalAlignment="Left" Height="28" Margin="61,74,0,0" TextWrapping="Wrap" Text="Начальное число" VerticalAlignment="Top" Width="174"/>
        <TextBox x:Name="EndTextBox" GotFocus="EndTextBox_GotFocus" HorizontalAlignment="Left" Height="28" Margin="61,142,0,0" TextWrapping="Wrap" Text="Конечное число" VerticalAlignment="Top" Width="174"/>
        <Button x:Name="GenerateButton" Click="GenerateButton_Click" Content="Button" HorizontalAlignment="Left" Margin="61,217,0,0" VerticalAlignment="Top" Width="174" Height="47"/>
        <ListBox x:Name="NumberListBox" HorizontalAlignment="Left" Height="190" Margin="317,74,0,0" VerticalAlignment="Top" Width="408"/>

    </Grid>
</Window>
